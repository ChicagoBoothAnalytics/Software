export HOME=/mnt/home
cd ~
export PROGRAMS_DIR=/mnt/programs
mkdir -p $PROGRAMS_DIR
export SPARK_HOME=/usr/lib/spark
export CUDA_ROOT=$PROGRAMS_DIR/cuda-7.5
export NVVMIR_LIBRARY_DIR=$CUDA_ROOT/nvvm/libdevice
export OPENBLAS_DIR=$PROGRAMS_DIR/OpenBLAS
export GEOS_DIR=/usr/local
export PROJ_DIR=/usr/local
export TMPDIR=/mnt/tmp
mkdir -p $TMPDIR
export LINUXBREW_HOME=$PROGRAMS_DIR/LinuxBrew
export HOMEBREW_TEMP=$TMPDIR
export PATH=$CUDA_ROOT/bin:$CUDA_ROOT/nvvm/bin:$LINUXBREW_HOME/bin:$LINUXBREW_HOME/sbin:$PATH
export LD_LIBRARY_PATH=$CUDA_ROOT/lib64:$CUDA_ROOT/lib
export KERNEL_RELEASE=$(uname -r)
export KERNEL_SOURCE_PATH=/usr/src/kernels/$KERNEL_RELEASE
export JUPYTER_DIR=~/.jupyter
export GITHUB_REPO_RAW_PATH=https://raw.githubusercontent.com/ChicagoBoothML/Helpy/master/ChicagoBoothML_Helpy/AWS-EMR
export ENV_VARS_SCRIPT_NAME=.EnvVars
export SPARK_DEFAULTS_CONF_FILE_NAME=spark-defaults.conf
export JUPYTER_NOTEBOOK_CONFIG_FILE_NAME=jupyter_notebook_config.py
export THEANORC_SCRIPT_NAME=.theanorc